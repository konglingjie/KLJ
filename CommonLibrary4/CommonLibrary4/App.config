<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <sectionGroup name="CommonLibrary4">
      <section name="logging" type="CommonLibrary4.Logging.Configuration.LoggingConfigurationSection"/>
      <section name="weblog" type="CommonLibrary4.Weblog.Configuration.WeblogConfigurationSection"/>
      <section name="database" type="CommonLibrary4.Database.Configuration.DatabaseConfigurationSection"/>
    </sectionGroup>
  </configSections>

  <CommonLibrary4>

    <cache>
      <caches>
        <add regionName="default" default="true" type="CommonLibrary4.Cache.LocalCache,CommonLibrary4" />
        <add regionName="test2" type="CommonLibrary4.Cache.ICache,......" connectionString="172.0.0.1:6379;172.0.0.1:6380......"/>
      </caches>
    </cache>

    <logging lowLevel="Debug">
      <filters>
        <filter name="filter1" logLevel="Debug|Warning|Error" type="CommonLibrary4.Logging.Filter.LogFilter,CommonLibrary4"/>
        <filter name="filter2" logLevel="Trace|Info" categories="default,cty2"/>
      </filters>
      <writers>
        <writer name="writer1" filter="filter1" type="CommonLibrary4.Logging.Writer.TextLogWriter,CommonLibrary4">
          <params>
            <add name="maxsize" value="1MB"/>
            <add name="folder" value="c:\\test\sprlog\%year%%month%\debug%day%log.log"/>
          </params>
        </writer>
        <writer name="writer2" filter="filter2" type="CommonLibrary4.Logging.Writer.TextLogWriter,CommonLibrary4">
          <params>
            <add name="maxsize" value="1MB"/>
            <add name="folder" value="c:\\test\sprlog\%year%%month%\%day%log.log"/>
          </params>
        </writer>
      </writers>
    </logging>

    <weblog version="1.0">
      <ignores>
        <ignore name="ig1" match="\.(js|css|jpg|png|gif|jpeg|exe|swf|pdf|xls|doc|xlsx|docx|txt|log)([\?&amp;][\w\w]*)?$"/>
      </ignores>
      <collect items="req_url,protocol,domain,absolute_path,query_string,req_type,user_agent,url_referrer,status_code,server_host,server_port,user_host,visit_time,sid,vuca,rewriter_url,load_time">
        <customs>
          <add name="vuca" type="CommonLibrary4.Weblog.Collect.ICollecter,......"/>
          <add name="sid" type="CommonLibrary4.Weblog.Collect.ICollecter,......"/>
        </customs>
      </collect>
      <writer type="CommonLibrary4.Weblog.Writer.TextWeblogWriter,CommonLibrary4">
        <params>
          <add name="maxsize" value="2MB"/>
          <add name="folder" value="c:\\test\sprweblog\%year%%month%\error%day%log.log"/>
        </params>
      </writer>
    </weblog>

    <database>
      <providers>
        <provider name="test">
          <builder type="CommonLibrary4.Database.SqlBuilder.SqlServerStatementBuilder,CommonLibrary4"/>
          <importer type="CommonLibrary4.Database.ImporterForSqlServer,CommonLibrary4"/>
        </provider>
      </providers>
    </database>
    
    <!--<crypto>
      <algorithms>
        <algorithm name="">
          <params>
            <add name="" value=""/>
            <add name="" value=""/>
          </params>
        </algorithm>
      </algorithms>
    </crypto>-->
  </CommonLibrary4>

</configuration>